{
  "name": "express-middleware",
  "version": "1.0.0",
  "main": "dist/app.js",
  "description": "express middleware to integrate helloauth based OAuth servers into nodejs applications",
  "keywords": [
    "helloauth",
    "express",
    "auth",
    "middleware",
    "oauth"
  ],
  "repository": "git@github.com:HelloAuth/express-middleware.git",
  "scripts": {
    "build": "tsc --outDir ./dist src/app.ts",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/app.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^15.0.0",
    "@types/node-fetch": "^2.5.8",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
